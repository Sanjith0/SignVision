{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "dest": "api/index.py"
    },
    {
      "src": "/models",
      "dest": "api/index.py"
    },
    {
      "src": "/analyze-fallback",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*\\.(js|css|json|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

